{
    "name": "pocketbase-sveltekit-auth",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "pnpm typegen && vite dev",
        "build": "vite build",
        "pocketbase": "cd backend && pocketbase serve",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "typegen": "npx pocketbase-typegen --db ./backend/pb_data/data.db --out src/lib/pocketbase-types.ts --env"
    },
    "devDependencies": {
        "@floating-ui/dom": "^1.5.2",
        "@playwright/test": "^1.38.0",
        "@skeletonlabs/skeleton": "^1.12.0",
        "@sveltejs/adapter-auto": "^2.1.0",
        "@sveltejs/kit": "^1.25.0",
        "@tailwindcss/forms": "^0.5.6",
        "@tailwindcss/typography": "^0.5.10",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "autoprefixer": "^10.4.15",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-svelte": "^2.33.1",
        "iconify-icon": "^1.0.8",
        "node-fetch": "^3.3.2",
        "pocketbase": "^0.16.0",
        "pocketbase-typegen": "^1.1.13",
        "postcss": "^8.4.29",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "sharp": "^0.32.5",
        "svelte": "^3.59.2",
        "svelte-check": "^3.5.1",
        "svelte-preprocess": "^5.0.4",
        "sveltekit-superforms": "^1.6.1",
        "tailwind-merge": "^1.14.0",
        "tailwindcss": "^3.3.3",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^4.4.9",
        "vitest": "^0.25.8",
        "zod": "^3.22.2"
    },
    "type": "module"
}